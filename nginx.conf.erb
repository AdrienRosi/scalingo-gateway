if ($http_apikey = "") {
  return 401;
}
if ($http_apikey != <%= ENV["API_KEY"] %>) {
  return 403;
}

location /repertoire {
  rewrite ^/repertoire(.*)$ $1 break;
  proxy_pass <%= ENV["API_V1_BACKEND"] %>;
  proxy_redirect default;
}

location /api/v1 {
  proxy_pass <%= ENV["API_V2_BACKEND"] %>;
}

return 503;
