if ($http_apikey = "") {
  return 401;
}
if ($http_apikey != <%= ENV["API_KEY"] %>) {
  return 403;
}
location /api/v1 {
  rewrite ^/api/v1(.*)$ / break;
  proxy_pass <%= ENV["API_V1_BACKEND"] %>;
  echo 'finally matched location /api/v1';
}

location /api/v2 {
  rewrite ^/api/v2(.*)$ / break;
  proxy_pass <%= ENV["API_V2_BACKEND"] %>;
  echo 'finally matched location /api/v2';
}

echo 'No match';

return 503;
